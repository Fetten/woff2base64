  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Fetten/woff2base64/issues"
  },
  "homepage": "https://github.com/Fetten/woff2base64#readme",
  "main": "./lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
